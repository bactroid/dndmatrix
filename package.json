  "repository": {
    "type": "git",
    "url": "git+https://github.com/bactroid/dndmatrix.git"
  },
  "bugs": {
    "url": "https://github.com/bactroid/dndmatrix/issues"
  },
  "homepage": "https://github.com/bactroid/dndmatrix#readme"
}
